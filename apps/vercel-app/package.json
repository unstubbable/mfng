{
  "name": "@mfng/vercel-app",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Hendrik Liebau <mail@hendrik-liebau.de>",
  "type": "module",
  "scripts": {
    "build": "NODE_OPTIONS='--import=tsx --conditions=@mfng:internal' webpack --mode production",
    "build-dev-server": "tsx dev-server/build.ts",
    "build-dev-server:dev": "tsx dev-server/build.ts",
    "build:dev": "NODE_OPTIONS='--import=tsx --conditions=@mfng:internal' webpack --mode development",
    "deploy": "vercel deploy --prebuilt",
    "dev": "npm run start",
    "start": "tsx watch --clear-screen=false --enable-source-maps --inspect dev-server/run.ts",
    "watch": "NODE_OPTIONS='--import=tsx --conditions=@mfng:internal' webpack --mode production --watch",
    "watch-dev-server": "tsx watch --clear-screen=false dev-server/watch.ts",
    "watch-dev-server:dev": "tsx watch --clear-screen=false dev-server/watch.ts",
    "watch:dev": "NODE_OPTIONS='--import=tsx --conditions=@mfng:internal' webpack --mode development --watch"
  },
  "dependencies": {
    "@mfng/core": "*",
    "@mfng/shared-app": "*",
    "@vercel/analytics": "^0.1.11",
    "react": "0.0.0-experimental-e3ebcd54b-20240405",
    "react-dom": "0.0.0-experimental-e3ebcd54b-20240405",
    "web-vitals": "^3.3.1"
  },
  "devDependencies": {
    "@hono/node-server": "^1.8.2",
    "@mfng/webpack-rsc": "*",
    "@swc/core": "^1.3.22",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "autoprefixer": "^10.4.14",
    "chokidar": "^3.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "cssnano": "^5.1.15",
    "esbuild": "^0.20.1",
    "hono": "^4.1.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "resolve-typescript-plugin": "^2.0.0",
    "source-map-loader": "^4.0.1",
    "swc-loader": "^0.2.3",
    "tailwindcss": "^3.2.7",
    "tsx": "^4.7.1",
    "vercel": "^28.18.3",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1",
    "webpack-manifest-plugin": "^5.0.0"
  }
}
